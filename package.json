{
  "name": "rk-word-editor-monorepo",
  "version": "3.2.1",
  "description": "A high-performance, modern rich-text editor monorepo built with Tiptap and ProseMirror",
  "private": true,
  "author": {
    "name": "RaaZ Khanal",
    "email": "raazkhnl@gmail.com",
    "url": "https://github.com/raazkhnl"
  },
  "license": "MIT",
  "keywords": [
    "editor",
    "tiptap",
    "prosemirror",
    "rich-text",
    "wysiwyg",
    "monorepo",
    "typescript"
  ],
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@raazkhnl/rk-editor-demo",
    "build": "npm run build --workspace=@raazkhnl/rk-editor-core && npm run build --workspace=@raazkhnl/rk-editor-ui",
    "build:all": "npm run build && npm run build --workspace=@raazkhnl/rk-editor-demo",
    "clean": "rimraf node_modules packages/*/node_modules packages/*/dist apps/*/node_modules apps/*/dist",
    "publish:packages": "npm publish --workspaces --access public"
  },
  "dependencies": {
    "@tiptap/core": "^2.27.0",
    "@tiptap/extension-color": "^2.27.0",
    "@tiptap/extension-font-family": "^2.27.0",
    "@tiptap/extension-highlight": "^2.27.0",
    "@tiptap/extension-image": "^2.27.0",
    "@tiptap/extension-subscript": "^2.27.0",
    "@tiptap/extension-superscript": "^2.27.0",
    "@tiptap/extension-table": "^2.27.0",
    "@tiptap/extension-table-cell": "^2.27.0",
    "@tiptap/extension-table-header": "^2.27.0",
    "@tiptap/extension-table-row": "^2.27.0",
    "@tiptap/extension-task-item": "^2.27.0",
    "@tiptap/extension-task-list": "^2.27.0",
    "@tiptap/extension-text-align": "^2.27.0",
    "@tiptap/extension-text-style": "^2.27.0",
    "@tiptap/extension-underline": "^2.27.0",
    "@tiptap/pm": "^2.27.0",
    "@tiptap/starter-kit": "^2.27.0",
    "docx": "^9.6.0",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  }
}