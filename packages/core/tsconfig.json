{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "outDir": "./dist/types",
        "rootDir": "./src",
        "noEmit": false,
        "declaration": true,
        "declarationDir": "./dist",
        "emitDeclarationOnly": true,
        "skipLibCheck": true,
        "allowImportingTsExtensions": true
    },
    "include": [
        "src"
    ]
}